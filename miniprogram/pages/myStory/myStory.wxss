/* pages/myStory/myStory.wxss */
.plus-circle {
  position: fixed;
  bottom: 70px;
  right: 20px;
  display: inline-block;
  width: 50px;
  height: 50px;
  line-height: 68px;
  text-align: center;
  border-radius: 25px;
  background: #3A96E7;
  color: #fff;
  z-index: 9999;
}
.delete-cell {
  background: red;
  text-align: center;
  color: #fff;
}
.van-swipe-cell__left,
.van-swipe-cell__right {
  display: inline-block;
  width: 65px;
  height: 44px;
  font-size: 15px;
  line-height: 44px;
  color: #fff;
  text-align: center;
  background-color: #ee0a24;
}