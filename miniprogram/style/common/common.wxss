.ellipsis {
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}
.ellipsis-multi-2 {
  display: -webkit-box;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
  overflow: hidden;
}
.ellipsis-multi-3 {
  display: -webkit-box;
  -webkit-line-clamp: 3;
  -webkit-box-orient: vertical;
  overflow: hidden;
}
.item-icon {
  width: 40rpx;
  height: 40rpx;
}
.my-btn {
  width: auto;
  padding: 0;
  font-weight: 400;
  border-radius: 0;
}
.my-btn.button-hover,
.navigator-hover {
  background-color: rgba(128, 128, 128, 0.1);
  opacity: 0.7;
}
.center {
  text-align: center;
}
page {
  background: #f5f5f5;
}
